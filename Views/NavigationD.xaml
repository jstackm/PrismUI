<UserControl x:Class="PrismUI.Views.NavigationD"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <StackPanel HorizontalAlignment="Left"
                    VerticalAlignment="Top">
            <Label Content="View D"
                   FontSize="20"/>
            <TextBlock Margin="5"
                       FontSize="14">
                View Transition with  <LineBreak />
                Parameters Return to Main View
            </TextBlock>

            <TextBox Text="{Binding ViewDText}"
                       Margin="5"
                       FontSize="18"/>

            <Button Content="Return Parameters to Main View"
                    Margin="3"
                    Command="{Binding ReturnParametersToMainView}"/>
        </StackPanel>
            
    </Grid>
</UserControl>
